<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<constant name="struts.devMode" value="true" />

	<package name="userManage" extends="struts-default">

		<action name="login" class="action.UserAction" method="login">
			<result name="input">/page/login.jsp</result>
			<result name="success">/page/success.jsp</result>
		</action>
		
		<action name="regist" class="action.UserAction" method="regist">
			<result name="input">/page/regist.jsp</result>
			<result name="success">/page/success.jsp</result>
		</action>
	</package>
	
</struts>